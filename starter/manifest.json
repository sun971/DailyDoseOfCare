{
  "name": "Daily Dose of Care",
  "version": "0.0.1",
  "description": "Simple self care extension to keep you on track ❤️",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["tabs", "activeTab", "declarativeContent", "storage"],
  "icons":{
    "16": "icons/logo.png.png",
    "128": "icons/logo.png.png"

  }
  
}
