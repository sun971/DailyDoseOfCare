{
  "name": "Dose of Sunshine",
  "version": "0.0.1",
  "description": "Simple self care extension to keep you on track <3",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["tabs", "activeTab", "declarativeContent", "storage"]
}
